<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration 
    PUBLIC "-//mybatis.org//DTD SQL Map Config 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-config.dtd"> 
<configuration> 
	<settings>
		<setting name="callSettersOnNulls" value="true" />
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>

	<typeAliases>
		<typeAlias alias="LicenseDto" type="com.jl.seastory.product.dto.LicenseDto"/>
		<typeAlias alias="MemberDto" type="com.jl.seastory.member.dto.MemberDto"/>
	</typeAliases>

	<mappers>
		<mapper resource="MemberMapper.xml" />
		<mapper resource="LicenseMapper.xml" />
	</mappers>
</configuration>